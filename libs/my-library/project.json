{
  "name": "my-library",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/my-library/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/libs/my-library",
        "tsConfig": "libs/my-library/tsconfig.json",
        "main": "libs/my-library/src/index.ts",
        "index": "",
        "sourceMap": false,
        "generateIndexHtml": false,
        "webpackConfig": "libs/my-library/webpack.config.js",
        "compiler": "babel",
        "assets": [],
        "external": ["react/jsx-runtime"]
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": false,
          "vendorChunk": true
        },
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": false,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/react:module-federation-dev-server",
      "outputs": [
        "{options.outputFile}"
      ],
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "my-library:build",
        "hmr": true,
        "watch": true,
        "liveReload": true,
        "port": 7001
      },
      "configurations": {
        "development": {
          "buildTarget": "my-library:build:development"
        },
        "production": {
          "buildTarget": "my-library:build:production",
          "hmr": false
        }
      }
    },
    "serve-static": {
      "executor": "@nrwl/web:file-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "my-library:build",
        "port": 7001
      },
      "configurations": {
        "development": {
          "buildTarget": "my-library:build:development"
        },
        "production": {
          "buildTarget": "my-library:build:production"
        }
      }
    }
  },
  "tags": []
}